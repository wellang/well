~macro:move[2] {
               asm~ mov %2, %1
}

~macro:pop[1] {
              asm~ pop %1
}

~macro:add[2] {
              asm~ add %2, %1
}

~macro:sub[2] {
              asm~ sub %2, %1
}

~macro:xor[2] {
              asm~ xor %2, %1
}


