~include <std.well>

~var:main {
	string~ test = 'test'
	string~ run_test = 'run test'
}

~func:print {

	move~ 1, rax
	move~ 1, rdi
	move~ r8, rsi
	move~ r9, rdx
	syscall

	asm~ ret

}

~func:run {

	move~ run_test, r8
	move~ run_test.len, r9
	return~ 0

}

~func:main {

	move~ test, r8
	move~ test.len, r9
	call~ print

	call~ run	

}

